class Solution {
  public:
    
    DLLNode* reverseDLL(DLLNode* head) {
        if (!head || !head->next) return head; 
        
        DLLNode* curr = head;
        DLLNode* temp = nullptr;
        
    
        while (curr) {
            temp = curr->prev;
            curr->prev = curr->next;
            curr->next = temp;
            
           
            curr = curr->prev;
        }
        
       
        if (temp) {
            head = temp->prev;
        }
        
        return head;
    }
};
